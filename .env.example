# Langfuse Self-Hosted Credentials
# Copy to .env and run: ./scripts/generate-env.sh

# PostgreSQL Database Password
# Used by Langfuse to store traces, users, and projects
POSTGRES_PASSWORD=CHANGE_ME

# Encryption Key (64 hex characters)
# Used to encrypt sensitive data at rest
ENCRYPTION_KEY=CHANGE_ME_64_HEX_CHARS

# NextAuth Secret (64 hex characters)
# Used for session token encryption
NEXTAUTH_SECRET=CHANGE_ME_64_HEX_CHARS

# Salt (32 hex characters)
# Used for password hashing
SALT=CHANGE_ME_32_HEX_CHARS

# ClickHouse Database Password
# Used for analytics queries and aggregations
CLICKHOUSE_PASSWORD=CHANGE_ME

# MinIO Root Password
# MinIO provides S3-compatible object storage for media and exports
MINIO_ROOT_PASSWORD=CHANGE_ME

# Redis Authentication
# Redis is used for caching and job queues
REDIS_AUTH=CHANGE_ME

# Langfuse Init Project Keys
# Auto-created "Claude Code" project credentials
LANGFUSE_INIT_PROJECT_PUBLIC_KEY=pk-lf-local-claude-code
LANGFUSE_INIT_PROJECT_SECRET_KEY=CHANGE_ME

# Langfuse Init User
# Admin user auto-created on first boot
LANGFUSE_INIT_USER_EMAIL=your@email.com
LANGFUSE_INIT_USER_NAME=Your Name
LANGFUSE_INIT_USER_PASSWORD=change-me-on-first-login

# Langfuse Organization Name (optional)
# Customize your organization name
LANGFUSE_INIT_ORG_NAME=My Org
